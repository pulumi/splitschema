                "type": "object"
            },
            "validFromDate": {
                "description": "The beginning date that the certificate is valid.\n",
                "type": "string"
            },
            "validToDate": {
                "description": "The final date that the certificate is valid.\n",
                "type": "string"
            }
        },
        "required": [
            "certificateArn",
            "certificateCreationDate",
            "certificateId",
            "certificateOwner",
            "certificatePem",
            "certificateWallet",
