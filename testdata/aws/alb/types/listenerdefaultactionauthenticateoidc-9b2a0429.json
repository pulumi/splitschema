{
    "properties": {
        "authenticationRequestExtraParams": {
            "type": "object",
            "additionalProperties": {
                "type": "string"
            },
            "description": "Query parameters to include in the redirect request to the authorization endpoint. Max: 10.\n"
        },
        "authorizationEndpoint": {
            "type": "string",
            "description": "Authorization endpoint of the IdP.\n"
        },
        "clientId": {
            "type": "string",
            "description": "OAuth 2.0 client identifier.\n"
        },
        "clientSecret": {
            "type": "string",
            "description": "OAuth 2.0 client secret.\n",
            "secret": true
        },
        "issuer": {
            "type": "string",
